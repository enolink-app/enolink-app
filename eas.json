{
    "cli": {
        "version": ">= 16.13.4",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {
            "autoIncrement": true,
            "android": {
                "buildType": "app-bundle"
            },
            "env": {
                "APP_ENV": "production"
            }
        },
        "test": {
            "env": {
                "APP_ENV": "test"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
